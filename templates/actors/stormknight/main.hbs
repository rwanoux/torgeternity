<form class="actor-sheet {{cssClass}}" autocomplete="off">

    <nav class="sheet-tabs tabs" data-group="primary">
        <a class="item" data-tab="stats" style="vertical-align:center">Stats</a>
        <a class="item" data-tab="perks">Perks</a>
        <a class="item" data-tab="gear">Gear</a>
        <a class="item" data-tab="powers">Powers</a>
        <a class="item" data-tab="background">Background</a>
    </nav>

    <header class="sheet-header block">
        {{> "systems/torgeternity/templates/actors/stormknight/header.hbs"}}

    </header>

    <div class="attributes block " data-group="primary" data-tab="stats">
        {{> "systems/torgeternity/templates/actors/stormknight/attributes.hbs"}}
    </div>

    <div class="skills block" data-group="primary" data-tab="stats">
        {{> "systems/torgeternity/templates/actors/stormknight/skills.hbs"}}
    </div>
    <div class="char_image">
        <img src="{{actor.img}}" class="profile-img" data-edit="img" title="{{actor.name}}" />

    </div>
    <div class="perks block">
        {{> "systems/torgeternity/templates/actors/stormknight/perks.hbs"}}
    </div>
     <div class="defense block">
        {{> "systems/torgeternity/templates/actors/stormknight/defense.hbs"}}
    </div>
</form>